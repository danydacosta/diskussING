{
  "description": "A chat that uses the Diskuss API.",
  "manifest_version": 2,
  "name": "diskussING",
  "version": "1.0",
  "homepage_url": "https://github.com/danydacosta/diskussING",
  "icons": {
    "48": "icons/diskuss-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/diskuss-32.png",
    "default_title": "diskussING",
    "default_popup": "popup/index.html"
  }
}
